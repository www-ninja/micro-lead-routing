{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "validateWeight": true,
  "properties": {
    "routes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "route_id": {
            "type": "integer",
            "minimum": 1
          },
          "order": {
            "type": "integer",
            "minimum": 1
          }
        }
      }
    }
  }
}
